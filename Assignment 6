#include<iostream>
#include<fstream>
#include<sstream>
#include<cstdlib>
using namespace std;

void type()
{
  string filename;
  ifstream file;
  cin >> filename;
  file.open(filename);
  if (!file.is_open())
  {
    cout << "file does not exist";
  }
}
int main()
{
  cout << "Enter a filename";
  type();
}
